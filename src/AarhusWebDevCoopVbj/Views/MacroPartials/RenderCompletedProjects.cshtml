@inherits Umbraco.Web.Macros.PartialViewMacroPage
	@{ var selection = Umbraco.Content(1066).Children.OrderBy("createDate descending");}
	@*CurrentPage.Children.Where("Visible").OrderBy("CreateDate desc"); }*@
@* OrderBy() takes the property to sort by and optionally order desc/asc *@

  <h3>2 latest completed projects</h3>
<ul>
    @foreach (var item in selection.Take(2))
    {
		if(item.projectStatus == ("Completed")){
        	<li>
				<a href="@item.Url">@item.Name - @item.CreateDate</a>
				</li>	
		}
    }
</ul>